'atom-workspace':
  'ctrl-p': 'fuzzy-files:toggle'

'.fuzzy-files atom-text-editor[mini]':
  'enter': 'select-list:open'
  'alt-enter': 'select-list:open-externally'
  'ctrl-enter': 'select-list:show-in-folder'
  'alt-delete': 'select-list:trash'
  'alt-left': 'select-list:split-left'
  'alt-right': 'select-list:split-right'
  'alt-up': 'select-list:split-up'
  'alt-down': 'select-list:split-down'
  'alt-v alt-p': 'select-list:insert-p'
  'alt-v alt-a': 'select-list:insert-a'
  'alt-v': 'select-list:insert-r'
  'alt-v alt-r': 'select-list:insert-r'
  'alt-v alt-n': 'select-list:insert-n'
  'alt-c alt-p': 'select-list:copy-p'
  'alt-c alt-a': 'select-list:copy-a'
  'alt-c': 'select-list:copy-r'
  'alt-c alt-r': 'select-list:copy-r'
  'alt-c alt-n': 'select-list:copy-n'
  'alt-0': 'select-list:default-slash'
  'alt-/': 'select-list:forward-slash'
  'alt-\\': 'select-list:backslash'
  'alt-q': 'select-list:query-item'
  'alt-s': 'select-list:query-selection'
  'alt-f': 'select-list:claude-chat'
  'f5': 'select-list:update'

'.platform-win32 .fuzzy-files atom-text-editor[mini]':
  'alt-w alt-x': 'select-list:cut-file'
  'alt-w alt-c': 'select-list:copy-file'
