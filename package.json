{
  "name": "fuzzy-files",
  "main": "./lib/main",
  "version": "1.2.0",
  "description": "Quickly find and take an action over project files",
  "homepage": "https://github.com/bacadra/pulsar-fuzzy-files",
  "url": "https://github.com/bacadra/pulsar-fuzzy-files",
  "repository": "https://github.com/bacadra/pulsar-fuzzy-files",
  "license": "MIT",
  "engines": {
    "atom": ">=1.100.0 <2.0.0"
  },
  "consumedServices": {
    "atom.file-icons": {
      "versions": {
        "1.0.0": "consumeClassIcons"
      }
    }
  },
  "providedServices": {
    "autocomplete.provider": {
      "versions": {
        "4.0.0": "getProvider"
      }
    }
  },
  "activationCommands": {
    "atom-workspace": [
      "fuzzy-files:toggle"
    ]
  },
  "dependencies": {
    "atom-select-list": "^0.8.1",
    "diacritic": "0.0.2",
    "etch": "^0.14.1",
    "glob": "^10.3.3",
    "minimatch": "^5.0.1"
  }
}
